{
  "name": "addis-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios"
  },
  "dependencies": {
    "expo": "~54.0.0",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-local-authentication": "~14.0.1",
    "expo-notifications": "~0.28.16",
    "expo-clipboard": "~6.0.3",
    "expo-sharing": "~12.0.1",
    "expo-contacts": "~13.0.5",
    "expo-sms": "~12.0.1",
    "expo-device": "~6.0.2",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linear-gradient": "~13.0.2",
    "expo-blur": "~13.0.2",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.13.4",
    "expo-location": "~17.0.1",
    "expo-camera": "~15.0.16",
    "expo-barcode-scanner": "~13.0.1",
    "expo-av": "~14.0.7",
    "expo-video": "~1.2.7",
    "expo-audio": "~0.2.5",
    "expo-maps": "~0.2.2",
    "expo-background-task": "~0.1.4",
    "expo-file-system": "~17.0.1",
    "expo-sqlite": "~14.0.6",
    "expo-updates": "~0.25.27",
    "expo-app-integrity": "~0.1.4",
    "expo-blob": "~0.1.4",
    "expo-ui": "~0.1.4",
    "react-native-paper": "^5.12.3",
    "react-native-vector-icons": "^10.1.0",
    "axios": "^1.7.7",
    "react-query": "^3.39.3",
    "zustand": "^4.5.5",
    "date-fns": "^3.6.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "15.2.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-flash-message": "^0.4.2",
    "react-native-toast-message": "^2.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-qrcode-svg": "^6.3.1",
    "react-native-share": "^10.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.1.3",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.3.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "private": true,
  "expo": {
    "name": "Addis",
    "slug": "addis-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/addis-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.addis.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "com.addis.app",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://addis.com"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#1e3a8a",
          "defaultChannel": "default"
        }
      ],
      "expo-local-authentication",
      "expo-secure-store",
      "expo-updates"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": "https://addis.com"
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}